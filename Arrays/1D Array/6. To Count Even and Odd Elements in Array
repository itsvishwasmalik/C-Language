#include<stdio.h>
int main()
{
    int a[100],i,n,count1=0,count2=0;
    printf("Enter size of array: ");
    scanf("%d",&n);

    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }

    for(i=0;i<n;i++)
    {
        if(a[i]%2==0)
        {
            count1++;
        }
        else
        {
            count2++;
        }
    }
    printf("Number of Even Elements :%d\n",count1);
    printf("Number of Odd Elements :%d\n",count2);
    return 0;
}
