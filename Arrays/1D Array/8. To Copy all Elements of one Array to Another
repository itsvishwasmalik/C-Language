#include<stdio.h>
int main()
{
    int a[100],b[100],i,n,count=0;
    printf("Enter size of array: ");
    scanf("%d",&n);

    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }

    for(i=0;i<n;i++)
    {
        b[i]=a[i];
    }
    
    printf("Array1: ");

    for(i=0;i<n;i++)
    {
        printf("%d ",a[i]);
    }

    printf("\nArray2: ");

    for(i=0;i<n;i++)
    {
        printf("%d ",b[i]);
    }

    printf("\n");

    return 0;
}
