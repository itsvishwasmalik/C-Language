#include<stdio.h>
int main()
{
    int a[100],i,n,count=0;
    printf("Enter size of array: ");
    scanf("%d",&n);

    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }

    for(i=0;i<n;i++)
    {
        if(a[i]<0)
        {
            count++;
        }
    }

    printf("Number of Negative Elements :%d\n",count);
    return 0;
}
