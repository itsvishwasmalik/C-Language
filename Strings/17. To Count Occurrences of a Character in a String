#include <stdio.h>
#include <string.h>
#include <stdlib.h>

int main()
{
    char s[100],ch;
    int i,count=0;
    
    fgets(s,sizeof(s),stdin);
    scanf("%c",&ch);

    for(i=0;s[i]!='\0';i++)
    {
        if(s[i]==ch)
        {
            count++;
        }
    }

    printf("%d",count);

    return 0;
}
