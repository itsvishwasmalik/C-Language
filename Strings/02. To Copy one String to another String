#include <stdio.h>
#include<string.h>
#include<stdlib.h>

int main()
{
    char s[30],t[30];
    int i=0;
    fgets(s,sizeof(s),stdin);

    while(s[i]!='\0')
    {
        t[i]=s[i];
        i++;
    }

		/*
		for(i=0;s[i]!=0;i++)
		{
				t[i]=s[i];
		}
		*/

    t[i]='\0';
    puts(t);

    return 0;
}
